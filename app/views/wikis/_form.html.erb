
<%= form_for @wiki do |f| %>

<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :body %>
<%= f.text_area :body %>

<% if current_user.admin? || current_user.premium? %>
<%= f.label :private, "This is a Private Wiki" %>
<%= f.check_box :private %>
<% end %>
<%= f.submit "Save" %>

<% end %>